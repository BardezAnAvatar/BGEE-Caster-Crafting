    ACTION_IF ($wizard(7) < min_arcane_7) THEN BEGIN OUTER_SET ~min_arcane_7~ = $wizard(7) END
    ACTION_IF ($wizard(8) < min_arcane_8) THEN BEGIN OUTER_SET ~min_arcane_8~ = $wizard(8) END
    ACTION_IF ($wizard(9) < min_arcane_9) THEN BEGIN OUTER_SET ~min_arcane_9~ = $wizard(9) END
    ACTION_IF ($wizard(~A~) < min_arcane_A) THEN BEGIN OUTER_SET ~min_arcane_A~ = $wizard(A) END

    LAUNCH_ACTION_FUNCTION ~fn-read_class_spell_2DA~
        STR_VAR
            ~2DA~ = "MXSPLDRU"
