    OUTER_SET ~min_nature_2~ = 9999
    OUTER_SET ~min_nature_3~ = 9999
    OUTER_SET ~min_nature_4~ = 9999
    OUTER_SET ~min_nature_5~ = 9999
    OUTER_SET ~min_nature_6~ = 9999
    OUTER_SET ~min_nature_7~ = 9999
