    OUTER_SET ~min_arcane_5~ = 9999
    OUTER_SET ~min_arcane_6~ = 9999
    OUTER_SET ~min_arcane_7~ = 9999
    OUTER_SET ~min_arcane_8~ = 9999
    OUTER_SET ~min_arcane_9~ = 9999
