        ~Divine~, min_divine_1, min_divine_2, min_divine_3, min_divine_4, min_divine_5, min_divine_6, min_divine_7, min_divine_8, min_divine_9, 9999, min_divine_Q => ~Divine~
        ~Nature~, min_nature_1, min_nature_2, min_nature_3, min_nature_4, min_nature_5, min_nature_6, min_nature_7, min_nature_8, min_nature_9, 9999, min_nature_Q => ~Nature~
    END

END
